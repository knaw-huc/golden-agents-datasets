    #####################################################################
    # ROAR++ Ontology and Thesaurus part
    #####################################################################

    # Ontology: Event
    roar:Event a owl:Class .
    roar:EventType a owl:Class .

    roar:RegistrationEvent a owl:Class;
        rdfs:subClassOf roar:Event .
    roar:RegistrationEventType a owl:Class;
        rdfs:subClassOf roar:EventType .

    # Ontology: Document
    roar:Document a owl:Class .
    roar:DocumentType a owl:Class .

    # Ontology: Role
    roar:Role a owl:Class .
    roar:RoleType a owl:Class .

    # Thesaurus: EventTypes
    thes:Doop a roar:EventType ;
        rdfs:label "Doop" ;
        rdfs:subClassOf roar:RegistrationEvent .

    thes:Ondertrouw a roar:EventType ;
        rdfs:label "Ondertrouw" ;
        rdfs:subClassOf roar:RegistrationEvent .

    thes:Begraven a roar:EventType ;
        rdfs:label "Begraven" ;
        rdfs:subClassOf roar:RegistrationEvent .

    # Thesaurus: RoleTypes
    thes:Geregistreerde a roar:RoleType ;
        rdfs:label "Geregistreerde" ;
        rdfs:subClassOf roar:Role .

    thes:Bruid a roar:RoleType ;
        rdfs:label "Bruid" ;
        rdfs:subClassOf roar:Role .

    thes:Bruidegom a roar:RoleType ;
        rdfs:label "Bruidegom" ;
        rdfs:subClassOf roar:Role .

    thes:Getuige a roar:RoleType ;
        rdfs:label "Getuige" ;
        rdfs:subClassOf roar:Role .

    thes:Vader a roar:RoleType ;
        rdfs:label "Vader" ;
        rdfs:subClassOf roar:Role .

    thes:Moeder a roar:RoleType ;
        rdfs:label "Moeder" ;
        rdfs:subClassOf roar:Role .

    thes:Kind a roar:RoleType ;
        rdfs:label "Kind" ;
        rdfs:subClassOf roar:Role .

    # Thesaurus: DocumentTypes
    thes:DtbDopen a roar:DocumentType ;
        rdfs:label "DTB Dopen" ;
        rdfs:subClassOf roar:Document .

    thes:Ondertrouwregister a roar:DocumentType ;
        rdfs:label "Ondertrouwregister" ;
        rdfs:subClassOf roar:Document .

    thes:DtbBegraven a roar:DocumentType ;
        rdfs:label "DTB Begraven" ;
        rdfs:subClassOf roar:Document .

    #####################################################################
    # Mapping #
    #####################################################################

    roar:Person          owl:equivalentClass       ga:Person .

    roar:Role            owl:equivalentClass       ga:Role .
    roar:RoleType        owl:equivalentClass       ga:RoleType .

    roar:Event           owl:equivalentClass       ga:Event .
    roar:EventType       owl:equivalentClass       ga:EventType .

    roar:Document        owl:equivalentClass       ga:Record .
    roar:DocumentType    owl:equivalentClass       ga:RecordType .

    roar:participatesIn  owl:equivalentProperty    ga:participatesIn .
    roar:carriedIn       owl:equivalentProperty    ga:carriedIn .
    roar:carriedBy       owl:equivalentProperty    ga:carriedBy .

    # ga:hasName         owl:equivalentProperty    pnv:hasName/pnv:literalName .
    ga:hasName           owl:propertyChainAxiom    ( pnv:hasName pnv:literalName ) .

    # thes:Doop            owl:subClassOf            ga:Event . # Already defined above
    # thes:Ondertrouw      owl:subClassOf            ga:Event . # Already defined above
    # thes:Begraven        owl:subClassOf            ga:Event . # Already defined above

    ga:hasXSDDate owl:equivalentProperty sem:hasTimeStamp .

